{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "EXPO_NO_DOTENV": "1"
        },
        "cache": {
          "key": "preview-v1"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_NO_DOTENV": "1"
        }
      }
    }
  }
}
