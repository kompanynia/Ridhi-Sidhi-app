{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "developmentClient": true,
        "distribution": "internal",
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "APP_VARIANT": "development"
        }
      },
      "ios": {
        "developmentClient": true,
        "distribution": "internal"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "distribution": "internal",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "EXPO_NO_DOTENV": "1"
        }
      },
      "ios": {
        "distribution": "internal"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_NO_DOTENV": "1"
        }
      },
      "ios": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
